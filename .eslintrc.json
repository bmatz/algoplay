{
    "extends": "trigo",
    "env": {
        "node": true,
        "jest": true
    },
    "parserOptions": {
        "sourceType": "script"
    },
    "rules": {
        "strict": [2, "global"],
        "no-underscore-dangle": 0,
        "object-curly-newline": 0,
        "prefer-destructuring": 0
    }
}
